{
  "name": "flavioro-dialog-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@apollo/react-hooks": "^3.1.1",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.8",
    "@types/react-transition-group": "^4.4.0",
    "dotenv": "^8.2.0",
    "graphql": "^14.5.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-image": "^4.0.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "4.0.2",
    "styled-components": "^5.2.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "prestart": "yarn gen && yarn build",
    "prestart:dev": "yarn gen",
    "dev": "react-scripts start",
    "start": "npx serve -s build -p 3000",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "gen": "graphql-codegen --config codegen.yml",
    "client:codegen": "node ./packages/apollo/bin/run client:codegen --target=typescript --outputFlat ./packages/apollo-language-server/src/graphqlTypes.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.3",
    "@graphql-codegen/typescript": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@types/graphql": "^14.5.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/styled-components": "^5.1.7"
  }
}
